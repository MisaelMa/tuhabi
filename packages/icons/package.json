{
  "name": "@ammc/icons",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "storybook": "heft start --storybook",
    "test": "rushx test:web && rushx test:native",
    "test:web": "jest --config='./config/jest/jest.web.js' --coverage --runInBand --colors",
    "test:native": "jest --config='./config/jest/jest.native.js' --coverage --runInBand --colors",
    "test:ci": "rushx test:ci:web && rushx test:ci:native",
    "test:ci:web": "jest --config='./config/jest/jest.web.js' --ci --reporters=default --reporters=github-actions --coverage --colors",
    "test:ci:native": "jest --config='./config/jest/jest.native.js' --ci --reporters=default --reporters=github-actions --coverage --colors",
    "lint": "tsc --noEmit && eslint src"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "react-native-svg": "^12.3.0",
    "svgs": "^4.2.0",
    "@ammc/commons": "workspace:*",
    "react-native": "0.68.1",
    "styled-components": "^5.3.5",
    "react-native-web": "0.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@babel/runtime": "^7.12.5",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.0",
    "@rnx-kit/metro-config": "^1.2.33",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.21",
    "@rushstack/eslint-config": "^2.6.0",
    "@rushstack/heft-storybook-plugin": "^0.1.47",
    "@rushstack/heft": "^0.45.1",
    "@ammc/eslint-settings": "workspace:*",
    "@ammc/heft-storybook-web": "workspace:*",
    "@ammc/jest": "workspace:*",
    "@ammc/typescript-settings": "workspace:*",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react-native": "^10.1.1",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.11.3",
    "@types/react-dom": "^18.0.6",
    "@types/react-native": "^0.67.3",
    "@types/react": "^18.0.21",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.9.1",
    "eslint-config-next": "^12.3.1",
    "eslint": "^8.26.0",
    "jest": "^28.1.2",
    "metro-config": " ^0.71.0",
    "metro-react-native-babel-preset": "^0.71.0",
    "next": "^13.0.6",
    "react-is": "^18.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  }
}
